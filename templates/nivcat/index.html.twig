{% extends 'base.html.twig' %}

{% block title %}Nivcat index{% endblock %}

{% block body %}
    <h1>Nivcat index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for nivcat in nivcats %}
            <tr>
                <td>{{ nivcat.id }}</td>
                <td>
                    <a href="{{ path('nivcat_show', {'id': nivcat.id}) }}">show</a>
                    <a href="{{ path('nivcat_edit', {'id': nivcat.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('nivcat_new') }}">Create new</a>
{% endblock %}
